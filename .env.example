# ============================================================
# ОКО-Сервис: Конфигурация
# Скопируйте этот файл в .env и заполните своими значениями
# ============================================================

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Video Source
RTSP_URL=rtsp://192.168.1.100:554/stream
# Или для локальной камеры (0 = встроенная веб-камера)
CAMERA_ID=0

# Model Configuration
MODEL_PATH=models/yolox_nano.onnx
CONFIDENCE_THRESHOLD=0.5

# Detection Settings
CHECK_INTERVAL=5  # Проверка каждые 5 секунд
FPS=15            # Кадров в секунду для обработки

# Logging
LOG_LEVEL=INFO    # DEBUG, INFO, WARNING, ERROR
